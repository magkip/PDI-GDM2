hema.lims_eu&#x7d;.x_meanings xm&#xd;&#xa;			on tf.x_meaning &#x3d; xm.name&#xd;&#xa;		join &#x24;&#x7b;mxns.gdw.schema.gdw_stage&#x7d;.map_meanings mm&#xd;&#xa;			on xm.name &#x3d; mm.source_key1&#xd;&#xa;			and mm.source_key2 &#x3d; &#x27;ORDER&#x27;&#xd;&#xa;			and mm.source_instance &#x3d; &#x27;&#x24;&#x7b;mxns.gdw.source_instance.eu&#x7d;&#x27;&#xd;&#xa;		left join &#x24;&#x7b;mxns.gdw.db.lims_eu&#x7d;.&#x24;&#x7b;mxns.gdw.schema.lims_eu&#x7d;.list_entry le&#xd;&#xa;			on le.list &#x3d; &#x27;LANGUAGES&#x27;&#xd;&#xa;			and nvl&#x28;tf.list_name, fm.list_key&#x29; like &#x27;_&#x5c;&#x5c;_&#x25;&#x27; escape &#x27;&#x5c;&#x5c;&#x27;&#xd;&#xa;			and substr&#x28;nvl&#x28;tf.list_name, fm.list_key&#x29;, 0, 2&#x29; &#x3d; le.name &#xd;&#xa;		-- below not dev&#xd;&#xa;		join &#x24;&#x7b;mxns.gdw.db.lims_au&#x7d;.&#x24;&#x7b;mxns.gdw.schema.lims_au&#x7d;.project_temp_fields ptf&#xd;&#xa;	on ptf.template &#x3d; pt.name&#xd;&#xa;	and pt.version &#x3d; ptf.version&#xd;&#xa;join &#x24;&#x7b;mxns.gdw.db.lims_au&#x7d;.&#x24;&#x7b;mxns.gdw.schema.lims_au&#x7d;.field_master fm &#xd;&#xa;   on ptf.field_name &#x3d; fm.field_name&#xd;&#xa;   and fm.table_name &#x3d; &#x27;PROJECT&#x27;&#xd;&#xa;join &#x24;&#x7b;mxns.gdw.db.lims_au&#x7d;.information_schema.columns c&#xd;&#xa;	on ptf.field_name &#x3d; c.column_name&#xd;&#xa;	and c.table_schema &#x3d; &#x27;&#x24;&#x7b;mxns.gdw.schema.lims_au&#x7d;&#x27;&#xd;&#xa;	and c.table_name &#x3d; &#x27;PROJECT&#x27;&#xd;&#xa;	&#x29;&#xd;&#xa;	select&#xd;&#xa;		gdw_template_id as template_id&#xd;&#xa;	,	field_name&#xd;&#xa;	, 	entry_mode&#xd;&#xa;	,	order_number&#xd;&#xa;	,	default_value&#xd;&#xa;	,	allow_user_entry&#xd;&#xa;	,	data_type&#xd;&#xa;	,	group_title as group_name&#xd;&#xa;	, 	list_code&#xd;&#xa;	,	max_length&#xd;&#xa;	,	max_length_type&#xd;&#xa;	,	gdw_meaning_id as meaning_id&#xd;&#xa;	,	is_reportable&#xd;&#xa;	,	is_downloadable&#xd;&#xa;	,	is_login_visible&#xd;&#xa;	, 	current_timestamp as updated_on&#xd;&#xa;	, 	&#x27;&#x24