    <sql>use &#x24;&#x7b;mxns.gdw.db.gdw&#x7d;.&#x24;&#x7b;mxns.gdw.schema.gdw&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;insert into E&#x24;_DIM_TEMPLATE_FIELDS&#xd;&#xa;&#x28;&#xd;&#xa;  error_ts&#xd;&#xa;, error_description&#xd;&#xa;, template_id&#xd;&#xa;, field_name&#xd;&#xa;, entry_mode&#xd;&#xa;, order_number&#xd;&#xa;, default_value&#xd;&#xa;, allow_user_entry&#xd;&#xa;, data_type&#xd;&#xa;, group_name&#xd;&#xa;, max_length&#xd;&#xa;, max_length_type&#xd;&#xa;, list_code&#xd;&#xa;, meaning_id&#xd;&#xa;, is_reportable&#xd;&#xa;, is_downloadable&#xd;&#xa;, is_login_visible&#xd;&#xa;, updated_on&#xd;&#xa;, updated_by&#xd;&#xa;, gdw_updated_on&#xd;&#xa;&#x29;&#xd;&#xa;with working_set as&#xd;&#xa;&#x28;&#xd;&#xa;	with source_template_fields as &#xd;&#xa;	&#x28;&#xd;&#xa;		select &#xd;&#xa;			mt.gdw_template_id&#xd;&#xa;		, 	tf.field_name&#xd;&#xa;		, 	tf.entry_mode&#xd;&#xa;		, 	tf.order_number&#xd;&#xa;		, 	tf.default_value&#xd;&#xa;		, 	tf.allow_user_entry&#xd;&#xa;		, 	tf.data_type&#xd;&#xa;		, 	tf.group_title&#xd;&#xa;		, 	case when le.name is not null then substr&#x28;nvl&#x28;tf.list_name, fm.list_key&#x29;, 3&#x29;&#xd;&#xa;				else nvl&#x28;tf.list_name, fm.list_key&#x29; &#xd;&#xa;			end as list_code&#xd;&#xa;		, 	c.character_octet_length as max_length&#xd;&#xa;		, 	case&#xd;&#xa;				when c.character_octet_length is not null then &#x27;BYTE&#x27; &#xd;&#xa;				else null&#xd;&#xa;			end as max_length_type&#xd;&#xa;		, 	mm.gdw_meaning_id&#xd;&#xa;		, 	nvl&#x28;tf.x_report_visible, &#x27;T&#x27;&#x29; as is_reportable&#xd;&#xa;		, 	nvl&#x28;tf.x_extraction_visible, &#x27;T&#x27;&#x29; as is_downloadable&#xd;&#xa;		,	case when tf.entry_mode in  &#x28;&#x27;MANDATORYENTRY&#x27; , &#x27;USERENTRY&#x27;&#x29; and tf.hidden_eli &#x3d; &#x27;F&#x27; then &#x27;T&#x27; else  &#x27;F&#x27; end as is_login_visible&#xd;&#xa;			from &#x24;&#x7b;mxns.gdw.db.lims_eu&#x7d;.&#x24;&#x7b;mxns.gdw.schema.lims_eu&#x7d;.sample_template st&#xd;&#xa;			join &#x24;&#x7b;mxns.gdw.schema.gdw_stage&#x7d;.map_templates mt&#xd;&#xa;				on st.name &#x3d; mt.template_code&#xd;&#xa;				and mt.template_type &#x3d; &#x27;SAMPLE&#x27;&#xd;&#xa;				and mt.source_instance &#x3d; &#x27;&#x24;&#x7b;mxns.gdw.source_instance.eu&#x7d;&#x27;&#xd;&#xa;			join &#x24;&#x7b;mxns.gdw.db.lims_eu&#x7d;.&#x24;&#x7b;mxns.gdw.schema.lims_eu&#x7d;.field_master fm &#xd;&#xa;				on fm.table_name &#x3d; &#x27;SAMPLE&#x27;&#xd;&#xa;			join &#x24;&#x7b;mxns.gdw.db.lims_eu&#x7d;.&#x24;&#x7b;mxns.gdw.schema.lims_eu&#x7d;.template_fields tf &#xd;&#xa;				on fm.field_name &#x3d; tf.field_name&#xd;&#xa;				and st.name &#x3d; tf.template &#xd;&#xa;			join &#x24;&#x7b;mxns.gdw.db.lims_eu&#x7d;.&#x24;&#x7b;mxns.gdw.schema.lims_eu&#x7d;.x_meanings xm&#xd;&#xa;				on tf.x_meaning &#x3d; xm.name&#xd;&#xa;			join &#x24;&#x7b;mxns.gdw.schema.gdw_stage&#x7d;.map_meanings mm&#xd;&#xa;				on xm.name &#x3d; mm.source_key1&#xd;&#xa;				and mm.source_key2 &#x3d; &#x27;SAMPLE&#x27;&#xd;&#xa;				and mm.source_instance &#x3d; &#x27;&#x24;&#x7b;mxns.gdw.source_instance.eu&#x7d;&#x27;&#xd;&#xa;			left join &#x24;&#x7b;mxns.gdw.db.lims_eu&#x7d;.&#x24;&#x7b;mxns.gdw.schema.lims_eu&#x7d;.list_entry le&#xd;&#xa;				on le.list &#x3d; &#x27;LANGUAGES&#x27;&#xd;&#xa;				and nvl&#x28;tf.list_name, fm.list_key&#x29; like &#x27;_&#x5c;&#x5c;_&#x25;&#x27; escape &#x27;&#x5c;&#x5c;&#x27;&#xd;&#xa;				and substr&#x28;nvl&#x28;tf.list_name, fm.list_key&#x29;, 0, 2&#x29; &#x3d; le.name&#xd;&#xa;			join &#x24;&#x7b;mxns.gdw.db.lims_au&#x7d;.information_schema.columns c&#xd;&#xa;				on tf.field_name &#x3d; c.column_name&#xd;&#xa;				and c.table_schema &#x3d; &#x27;&#x24;&#x7b;mxns.gdw.schema.lims_au&#x7d;&#x27; &#xd;&#xa;				and c.table_name &#x3d; &#x27;SAMPLES&#x27; -- source query has &#x28;ATC.TABLE_NAME&#x3d;&#x27;SAMPLE&#x27;&#x29;&#xd;&#xa;			left join &#x24;&#x7b;mxns.gdw.schema.gdw_stage&#x7d;.map_meanings mm &#xd;&#xa;				on tf.c_meaning &#x3d; mm.source_key1&#xd;&#xa;				and mm.source_key2 &#x3d; &#x27;SAMPLE&#x27;