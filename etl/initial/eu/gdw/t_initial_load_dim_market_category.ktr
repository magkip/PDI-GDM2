egory_name&#xd;&#xa;		,	market_category_desc&#xd;&#xa;		,	parent_market_category_id&#xd;&#xa;		,	&#x27;T&#x27; as is_active&#xd;&#xa;		, 	current_timestamp as updated_on&#xd;&#xa;		, 	&#x27;&#x24;&#x7b;mxns.gdw.etl_user&#x7d;&#x27; as updated_by&#xd;&#xa;		,	current_timestamp as gdw_updated_on&#xd;&#xa;		from source_mkt_category&#xd;&#xa;	&#x29;&#xd;&#xa;	select&#xd;&#xa;		w.market_category_id&#xd;&#xa;	,	w.market_category_name&#xd;&#xa;	,	w.market_category_desc&#xd;&#xa;	,	w.parent_market_category_id&#xd;&#xa;	,	w.is_active&#xd;&#xa;	, 	w.updated_on&#xd;&#xa;	,	w.updated_by&#xd;&#xa;	,	w.gdw_updated_on&#xd;&#xa;	from working_set w&#xd;&#xa;	join	&#xd;&#xa;	&#x28;&#xd;&#xa;		select market_category_id, parent_market_category_id&#xd;&#xa;		from working_set&#xd;&#xa;		group by market_category_id, parent_market_category_id&#xd;&#xa;		having count&#x28;&#x2a;&#x29; &#x3d; 1&#xd;&#xa;	&#x29; j on j.market_category_id &#x3d; w.market_category_id&#xd;&#xa;		and j.parent_market_category_id &#x3d; w.parent_market_category_id&#xd;&#xa;	where w.market_category_id is not null&#xd;&#xa;	and w.market_category_name is not null&#xd;&#xa;	and w.gdw_updated_on is not null&#xd;&#xa;	&#x29; as s on s.market_category_id &#x3d; d.market_category_id and s.parent_market_category_id &#x3d; d.parent_market_category_id&#xd;&#xa;when matched then update set&#xd;&#xa;  d.market_category_name &#x3d; s.market_category_name&#xd;&#xa;, d.market_category_desc &#x3d; s.market_category_desc&#xd;&#xa;, d.is_active &#x3d; s.is_active&#xd;&#xa;, d.updated_on &#x3d; s.updated_on&#xd;&#xa;, d.updated_by &#x3d; s.updated_by&#xd;&#xa;, d.gdw_updated_on &#x3d; s.gdw_updated_on&#xd;&#xa;when not matched then insert &#x28;&#xd;&#xa;  market_category_id&#xd;&#xa;, market_category_name&#xd;&#xa;, market_category_desc&#xd;&#xa;, parent_market_category_id&#xd;&#xa;, is_active&#xd;&#xa;, source_instance&#xd;&#xa;, updated_on&#xd;&#xa;, updated_by&#xd;&#xa;, gdw_updated_on&#xd;&#xa;&#x29;&#xd;&#xa;values&#xd;&#xa;&#x28;&#xd;&#xa;  s.market_category_id&#xd;&#xa;, s.market_category_name&#xd;&#xa;, s.market_category_desc&#xd;&#xa;, s.parent_market_category_id&#xd;&#xa;, s.is_active&#xd;&#xa;, s.source_instance&#xd;&#xa;, updated_on&#xd;&#xa;, updated_by&#xd;&#xa;, s.gdw_updated_on&#xd;&#xa;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;--only using JDE source data here&#x3f; &#xd;&#xa;	--NO, need to join LIMS and JDE&#xd;&#xa;	-- not populating dim_market_category for AU - only EU is populating this dimension&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;select DISTINCT&#xd;&#xa;	CAST&#x28;TRIM&#x28;FA.ABAC18&#x29;  AS VARCHAR2&#x28;6&#x29;&#x29;        C4_MKT_GLOBAL_IND_CODE,&#xd;&#xa;	CAST&#x28;TRIM&#x28;FC.DRDL01&#x29;  AS VARCHAR2&#x28;60&#x29;&#x29;        C5_MKT_GLOBAL_IND_DESC,&#xd;&#xa;	CAST&#x28;TRIM&#x28;FA.ABAC19&#x29;  AS VARCHAR2&#x28;10&#x29;&#x29;        C6_MKT_GEN_IND_CODE,&#xd;&#xa;	CAST&#x28;TRIM&#x28;FD.DRDL01&#x29;  AS VARCHAR2&#x28;60&#x29;&#x29;        C7_MKT_GEN_IND_DESC,&#xd;&#xa;	CAST&#x28;TRIM&#x28;FA.ABAC20&#x29;  AS VARCHAR2&#x28;10&#x29;&#x29;        C8_MKT_IND_SEG_CODE,&#xd;&#xa;	CAST&#x28;TRIM&#x28;FE.DRDL01&#x29;  AS VARCHAR2&#x28;60&#x29;&#x29;        C9_MKT_IND_SEG_DESC&#xd;&#xa;from &#xd;&#xa;&#x28;&#xd;&#xa;	&#x28;&#xd;&#xa;		&#x28;PRODCTL.F0005    FB &#xd;&#xa;			RIGHT OUTER JOIN PRODDTA.F0101    FA &#xd;&#xa;				ON TRIM&#x28;FB.DRKY&#x29;&#x3d;TRIM&#x28;FA.ABAC02&#x29; &#xd;&#xa;				AND TRIM&#x28;FB.DRRT&#x29; &#x3d; &#x27;02&#x27; &#xd;&#xa;				AND TRIM&#x28;FB.DRSY&#x29; &#x3d; &#x27;01&#x27; &#xd;&#xa;		&#x29; LEFT OUTER JOIN PRODCTL.F0005    FC &#xd;&#xa;			ON TRIM&#x28;FC.DRKY&#x29;&#x3d;TRIM&#x28;FA.ABAC18&#x29; &#xd;&#xa;			AND TRIM&#x28;FC.DRRT&#x29; &#x3d; &#x27;18&#x27; &#xd;&#xa;			AND TRIM&#x28;FC.DRSY&#x29; &#x3d; &#x27;01&#x27; &#xd;&#xa;	&#x29; &#xd;&#xa;	LEFT OUTER JOIN PRODCTL.F0005    FD &#xd;&#xa;		ON TRIM&#x28;FD.DRKY&#x29;&#x3d;TRIM&#x28;FA.ABAC19&#x29; &#xd;&#xa;		AND TRIM&#x28;FD.DRRT&#x29; &#x3d; &#x27;19&#x27; &#xd;&#xa;		AND TRIM&#x28;FD.DRSY&#x29; &#x3d; &#x27;01&#x27; &#xd;&#xa;&#x29; LEFT OUTER JOIN PRODCTL.F0005    FE &#xd;&#xa;	ON TRIM&#x28;FA.ABAC20&#x29; &#x3d; TRIM&#x28;FE.DRKY&#x29; &#xd;&#xa;	AND TRIM&#x28;FE.DRRT&#x29; &#x3d; &#x27;20&#x27; &#xd;&#xa;	AND TRIM&#x28;FE.DRSY&#x29; &#x3d; &#x27;01&#x27;&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;&#xd;&#xa;</sql>
