    <sql>use &#x24;&#x7b;mxns.gdw.db.gdw&#x7d;.&#x24;&#x7b;mxns.gdw.schema.gdw&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;insert into E&#x24;_DIM_MARKET_CATEGORY&#xd;&#xa;&#x28;&#xd;&#xa;  error_ts&#xd;&#xa;, error_description&#xd;&#xa;, market_category_id&#xd;&#xa;, market_category_name&#xd;&#xa;, market_category_desc&#xd;&#xa;, parent_market_category_id&#xd;&#xa;, is_active&#xd;&#xa;, source_instance&#xd;&#xa;, updated_on&#xd;&#xa;, updated_by&#xd;&#xa;, gdw_updated_on&#xd;&#xa;&#x29;&#xd;&#xa;with working_set as&#xd;&#xa;	&#x28;&#xd;&#xa;		with source_mkt_category as&#xd;&#xa;		&#x28;&#xd;&#xa;			-- INSERT SOURCE QUERY HERE&#xd;&#xa;&#x2f;&#x2a;select        &#xd;&#xa;    cast&#x28;trim&#x28;f0101.abac18&#x29;  as varchar2&#x28;6&#x29;&#x29; as c4_mkt_global_ind_code&#xd;&#xa;,   case when trim&#x28;f0005.drrt&#x29; &#x3d; &#x27;18&#x27; &#xd;&#xa;		and trim&#x28;f0005.drsy&#x29; &#x3d; &#x27;01&#x27;&#xd;&#xa;		then cast&#x28;trim&#x28;f0005.drdl01&#x29;  as varchar2&#x28;60&#x29;&#x29; &#xd;&#xa;	else null&#xd;&#xa;	end as c5_mkt_global_ind_desc&#xd;&#xa;,   cast&#x28;trim&#x28;f0101.abac19&#x29;  as varchar2&#x28;10&#x29;&#x29; as c6_mkt_gen_ind_code&#xd;&#xa;,   case when trim&#x28;f0005.drrt&#x29; &#x3d; &#x27;19&#x27; &#xd;&#xa;		and trim&#x28;f0005.drsy&#x29; &#x3d; &#x27;01&#x27;&#xd;&#xa;		then cast&#x28;trim&#x28;f0005.drdl01&#x29;  as varchar2&#x28;60&#x29;&#x29; &#xd;&#xa;	else null &#xd;&#xa;	end as c7_mkt_gen_ind_desc&#xd;&#xa;,   cast&#x28;trim&#x28;f0101.abac20&#x29;  as varchar2&#x28;10&#x29;&#x29; as c8_mkt_ind_seg_code&#xd;&#xa;,   case when trim&#x28;f0005.drrt&#x29; &#x3d; &#x27;20&#x27; &#xd;&#xa;		and trim&#x28;f0005.drsy&#x29; &#x3d; &#x27;01&#x27;&#xd;&#xa;		then cast&#x28;trim&#x28;f0005.drdl01&#x29;  as varchar2&#x28;60&#x29;&#x29; &#xd;&#xa;	else null &#xd;&#xa;	end as c9_mkt_ind_seg_desc&#xd;&#xa;from prodctl.f0101&#xd;&#xa;left join proddta.f0005&#xd;&#xa;	on trim&#x28;f0101.abac02&#x29; &#x3d; trim&#x28;f0005.drky&#x29; &#xd;&#xa;	and trim&#x28;f0005.drrt&#x29; &#x3d; &#x27;02&#x27; &#xd;&#xa;	and trim&#x28;f0005.drsy&#x29; &#x3d; &#x27;01&#x27; &#xd;&#xa;join stage.map_market_category mmc&#xd;&#xa;	on &#x2a;&#x2f;&#xd;&#xa;		&#x29;&#xd;&#xa;		select&#xd;&#xa;			gdw_market_category_id as market_category_id&#xd;&#xa;		,	market_category_name&#xd;&#xa;		,	market_category_desc&#xd;&#xa;		,	parent_market_category_id&#xd;&#xa;		,	&#x27;T&#x27; as is_active&#xd;&#xa;		, 	current_timestamp as updated_on&#xd;&#xa;		, 	&#x27;&#x24;&#x7b;mxns.gdw.etl_user&#x7d;&#x27; as updated_by&#xd;&#xa;		,	current_timestamp as gdw_updated_on&#xd;&#xa;		from source_mkt_category&#xd;&#xa;	&#x29;&#xd;&#xa;select current_timestamp, &#x27;duplicate record&#x27;, w.market_category_id, w.market_category_name, w.market_category_desc, w.parent_market_category_id, w.is_active, w.gdw_updated_on, w.gdw_delete_flg&#xd;&#xa;from working_set w&#xd;&#xa;join	&#xd;&#xa;&#x28;&#xd;&#xa;	select market_category_id, parent_market_category_id&#xd;&#xa;	from working_set&#xd;&#xa;	group by market_category_id, parent_market_category_id&#xd;&#xa;	having count&#x28;&#x2a;&#x29; &#x3d; 1&#xd;&#xa;&#x29; j on j.market_category_id &#x3d; w.market_category_id&#xd;&#xa;	and j.parent_market_category_id &#x3d; w.parent_market_category_id&#xd;&#xa;select current_timestamp, &#x27;market_category_id is null and field is mandatory&#x27;, w.market_category_id, w.market_category_name, w.market_category_desc, w.parent_market_category_id, w.is_active, w.gdw_updated_on, w.gdw_delete_flg&#xd;&#xa;from working_set w&#xd;&#xa;where market_category_id is null&#xd;&#xa;union&#xd;&#xa;select current_timestamp, &#x27;market_category_name is null and field is mandatory&#x27;, w.market_category_id, w.market_category_name, w.market_category_desc, w.parent_market_category_id, w.is_active, w.gdw_updated_on, w.gdw_delete_flg&#xd;&#xa;from working_set w&#xd;&#xa;where market_category_name is null&#xd;&#xa;union&#xd;&#xa;select current_timestamp, &#x27;updated_on is null and field is mandatory&#x27;, w.market_category_id, w.market_category_name, w.market_category_desc, w.parent_market_category_id, w.is_active, w.gdw_updated_on, w.gdw_delete_flg&#xd;&#xa;from working_set w&#xd;&#xa;where updated_on is null&#xd;&#xa;union&#xd;&#xa;select current_timestamp, &#x27;updated_by is null and field is mandatory&#x27;, w.market_category_id, w.market_category_name, w.market_category_desc, w.parent_market_category_id, w.is_active, w.gdw_updated_on, w.gdw_delete_flg&#xd;&#xa;from working_set w&#xd;&#xa;where updated_by is null&#xd;&#xa;&#x3b;</sql>
