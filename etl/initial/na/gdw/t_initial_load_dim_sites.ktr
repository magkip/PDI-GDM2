    <sql>use &#x24;&#x7b;mxns.gdw.db.gdw&#x7d;.&#x24;&#x7b;mxns.gdw.schema.gdw&#x7d;&#x3b;&#xd;&#xa;&#xd;&#xa;insert into E&#x24;_DIM_SITES&#xd;&#xa;&#x28;&#xd;&#xa;  error_ts&#xd;&#xa;, error_description&#xd;&#xa;, site_id&#xd;&#xa;, site_desc&#xd;&#xa;, country_code&#xd;&#xa;, zip&#xd;&#xa;, street_addr&#xd;&#xa;, addr_nr&#xd;&#xa;, city&#xd;&#xa;, state&#xd;&#xa;, latitude&#xd;&#xa;, longitude&#xd;&#xa;, is_active&#xd;&#xa;, lims_site_code&#xd;&#xa;, source_instance&#xd;&#xa;, updated_on&#xd;&#xa;, updated_by&#xd;&#xa;, street_addr2&#xd;&#xa;, cust_site_code&#xd;&#xa;, site_product_group&#xd;&#xa;, site_region&#xd;&#xa;, cust_site_code2&#xd;&#xa;, market_global_industry_id&#xd;&#xa;, market_genaral_industry_id&#xd;&#xa;, market_industry_segment_id&#xd;&#xa;, gdw_updated_on&#xd;&#xa;&#x29;&#xd;&#xa;with working_set as&#xd;&#xa;&#x28;&#xd;&#xa;	with source_sites as &#xd;&#xa;	&#x28;&#xd;&#xa;		select&#xd;&#xa;			ms.gdw_site_id as site_id&#xd;&#xa;		, 	el.description as site_desc&#xd;&#xa;		, 	el.country_code as country_code&#xd;&#xa;		, 	el.zip&#xd;&#xa;		, 	el.street_addr&#xd;&#xa;		,	el.addr_nr&#xd;&#xa;		, 	el.city&#xd;&#xa;		, 	el.state&#xd;&#xa;		, 	null as latitude&#xd;&#xa;		, 	null as longitude&#xd;&#xa;		, 	case&#xd;&#xa;				when el.removed &#x3d; &#x27;F&#x27; then &#x27;T&#x27;&#xd;&#xa;				else &#x27;F&#x27;&#xd;&#xa;			end as is_active&#xd;&#xa;		, 	el.name as lims_site_code&#xd;&#xa;		, 	&#x27;&#x24;&#x7b;mxns.gdw.source_instance.na&#x7d;&#x27; as source_instance&#xd;&#xa;		, 	el.changed_on as updated_on&#xd;&#xa;		, 	el.suite_bldg_desc as street_addr2&#xd;&#xa;		, 	el.facility_code as cust_site_code&#xd;&#xa;		, 	el.group_name as site_product_group&#xd;&#xa;		, 	el.prov_terr_code as site_region&#xd;&#xa;		, 	null as cust_site_code2&#xd;&#xa;		, 	null as market_global_industry_id&#xd;&#xa;		, 	null as market_genaral_industry_id&#xd;&#xa;		, 	null as market_industry_segment_id&#xd;&#xa;		from &#x24;&#x7b;mxns.gdw.db.lims_na&#x7d;.&#x24;&#x7b;mxns.gdw.schema.lims_na&#x7d;.external_location el&#xd;&#xa;		join &#x24;&#x7b;mxns.gdw.schema.gdw_stage&#x7d;.map_sites ms&#xd;&#xa;			on el.name &#x3d; ms.source_site_code&#xd;&#xa;			and ms.source_instance &#x3d; &#x27;&#x24;&#x7b;mxns.gdw.source_instance.na&#x7d;&#x27;&#xd;&#xa;	&#x29;&#xd;&#xa;	select&#xd;&#xa;		site_id&#xd;&#xa;	, 	site_desc&#xd;&#xa;	, 	country_code&#xd;&#xa;	, 	zip&#xd;&#xa;	, 	street_addr&#xd;&#xa;	, 	addr_nr&#xd;&#xa;	, 	city&#xd;&#xa;	, 	state&#xd;&#xa;	, 	latitude&#xd;&#xa;	, 	longitude&#xd;&#xa;	, 	is_active&#xd;&#xa;	, 	lims_site_code&#xd;&#xa;	, 	source_instance&#xd;&#xa;	, 	updated_on&#xd;&#xa;	,	&#x27;&#x7b;mxns.gdw.etl_user&#x7d;&#x27; as updated_by&#xd;&#xa;	, 	street_addr2&#xd;&#xa;	, 	cust_site_code&#xd;&#xa;	, 	site_product_group&#xd;&#xa;	, 	site_region&#xd;&#xa;	, 	cust_site_code2&#xd;&#xa;	, 	market_global_industry_id&#xd;&#xa;	, 	market_genaral_industry_id&#xd;&#xa;	, 	market_industry_segment_id&#xd;&#xa;	, 	current_timestamp as gdw_updated_on&#xd;&#xa;	from source_sites&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;select current_timestamp, &#x27;duplicate record&#x27;, w.site_id, w.site_desc, w.country_code, w.zip, w.street_addr, w.addr_nr, w.city, w.state, w.latitude, w.longitude, w.is_active, w.lims_site_code, w.source_instance, w.updated_on, w.updated_by, w.street_addr2, w.cust_site_code, w.site_product_group, w.site_region, w.cust_site_code2, w.market_global_industry_id, w.market_genaral_industry_id, w.market_industry_segment_id, w.gdw_updated_on&#xd;&#xa;from working_set w&#xd;&#xa;join&#xd;&#xa;&#x28;&#xd;&#xa;	select site_id&#xd;&#xa;	from working_set&#xd;&#xa;	group by site_id&#xd;&#xa;	having count&#x28;&#x2a;&#x29; &#x3e; 1&#xd;&#xa;&#x29; j on j.site_id &#x3d; w.site_id&#xd;&#xa;union&#xd;&#xa;select current_timestamp, &#x27;site_id is null and field is mandatory&#x27;, w.site_id, w.site_desc, w.country_code, w.zip, w.street_addr, w.addr_nr, w.city, w.state, w.latitude, w.longitude, w.is_active, w.lims_site_code, w.source_instance, w.updated_on, w.updated_by, w.street_addr2, w.cust_site_code, w.site_product_group, w.site_region, w.cust_site_code2, w.market_global_industry_id, w.market_genaral_industry_id, w.market_industry_segment_id, w.gdw_updated_on&#xd;&#xa;from working_set w&#xd;&#xa;where w.site_id is null&#xd;&#xa;union&#xd;&#xa;select current_timestamp, &#x27;updated_on is null and field is mandatory&#x27;, w.site_id, w.site_desc, w.country_code, w.zip, w.street_addr, w.addr_nr, w.city, w.state, w.latitude, w.longitude, w.is_active, w.lims_site_code, w.source_instance, w.updated_on, w.updated_by, w.street_addr2, w.cust_site_code, w.site_product_group, w.site_region, w.cust_site_code2, w.market_global_industry_id, w.market_genaral_industry_id, w.market_industry_segment_id, w.gdw_updated_on&#xd;&#xa;from working_set w&#xd;&#xa;where w.updated_on is null&#xd;&#xa;union&#xd;&#xa;select current_timestamp, &#x27;updated_by is null and field is mandatory&#x27;, w.site_id, w.site_desc, w.country_code, w.zip, w.street_addr, w.addr_nr, w.city, w.state, w.latitude, w.longitude, w.is_active, w.lims_site_code, w.source_instance, w.updated_on, w.updated_by, w.street_addr2, w.cust_site_code, w.site_product_group, w.site_region, w.cust_site_code2, w.market_global_industry_id, w.market_genaral_industry_id, w.market_industry_segment_id, w.gdw_updated_on&#xd;&#xa;from working_set w&#xd;&#xa;where w.updated_by is null&#xd;&#xa;&#x3b;</sql>
